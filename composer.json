{
    "name": "onefit/events",
    "description": "Events handling with kafka",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "ext-rdkafka": "^4.0",
        "ext-pcntl": "^7.2",
        "illuminate/support": "5.6.* || ^6.0",
        "ext-json": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "kwn/php-rdkafka-stubs": "^2.0",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "files": [
            "bootstrap/helpers.php"
        ],
        "psr-4": {
            "OneFit\\Events\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OneFit\\Events\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OneFit\\Events\\EventsServiceProvider"
            ]
        }
    },
    "license": "Proprietary",
    "minimum-stability": "stable"
}
