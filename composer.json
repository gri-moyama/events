{
    "name": "onefit/events",
    "description": "Event sourcing with kafka",
    "type": "library",
    "require": {
        "php": ">=7.1",
        "ext-rdkafka": "^4.0",
        "ext-pcntl": "^7.2",
        "illuminate/support": "5.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4",
        "kwn/php-rdkafka-stubs": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "OneFit\\Events\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OneFit\\Events\\EventsServiceProvider"
            ]
        }
    },
    "license": "Proprietary",
    "minimum-stability": "stable"
}
